{
  "expo": {
    "name": "Appname",
    "slug": "appname",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AppIcon.png",
    "scheme": "appname",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AppIcon.png",
        "backgroundColor": "#cc6d00"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.appname.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/AppIcon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "assets/images/AppIcon.png",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "url": "your-url",
      "webViewLoaderPath": "../assets/gif/AppLoader.gif",
      "appIconPath": "../assets/images/AppIcon.png",
      "themeColor": "#cc6d00ff",
      "notifications": false
    }
  }
}