{
  "expo": {
    "name": "CloudSPX",
    "slug": "cloudspx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/spx-ico.png",
    "scheme": "cloudspx",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/spx-ico.png",
        "backgroundColor": "#cc6d00"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.cloudspx.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/spx-ico.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "url": "https://stage.cloudspx.io/auth/login"
    }
  }
}